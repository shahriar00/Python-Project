      "source": [
        "import moviepy.editor\n",
        "from tkinter.filedialog import *\n",
        "\n",
        "vid = askopenfilename()\n",
        "video = moviepy.editor.VideoFileClip(vid)\n",
        "\n",
        "aud = video.audio\n",
        "aud.write_audiofile('demo.mp3')\n",
        "\n",
        "print(\"Convert Done!!\")"
